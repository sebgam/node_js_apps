<div id="insta_widget" class="row"></div>
<script>
$script(['https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js'],function(undefined){
  if(velma.feed.instagram!==undefined&&velma.feed.instagram!==null&&typeof velma.feed.instagram==='object'){
    $script(["instafeed.min.js",'https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.0.0-beta.3/owl.carousel.min.js'],function(){
      var obj={
        dots:false,
        clientId: 'a525e19ee1494f158f029a78de89369e',
        limit: 20,
        get: 'user', // added
        sortBy: 'most-liked',
        resolution:'low_resolution',
        template: '<a href="\{{link}}"><div class="background-setter insta-img" style="background-image:url(\{{image}});"></div></a>',
        after:function(){
          $("#instafeed").owlCarousel({
            margin:0,
            loop:true,
            autoWidth:true,
            items:6,
          });
        }
      }

      $.extend(obj,velma.feed.instagram);
      var feed = new Instafeed(obj);
      feed.run();

    });
    $("#insta_widget").append('<div class="col-lg-12"><div class="row"><div id="instafeed" class="owl-carousel"></div></div></div>');
  }
});
</script>
