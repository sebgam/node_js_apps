<div id="flickr_widget" class="row"></div>
<script>
$script(['https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js'],function(undefined){
  if(velma.feed.flickr!==undefined&&velma.feed.flickr!==null&&typeof velma.feed.flickr==='object'){
    $script(['https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js'],function(){
      $script(["jflickrfeed.min.js",'https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.3/owl.carousel.min.js'],function(){
        var obj={
          limit: 20,
          qstrings: {
            id: '139052977@N02'
          },
          itemTemplate: '<a href="\{{link}}"><div class="background-setter insta-img" style="background-image:url(\{{image}});"></div></a>'
        };
        $.extend(obj,velma.feed.flickr);
        $('#flickrfeed').jflickrfeed(obj,function(){
          $("#flickrfeed").owlCarousel({
            margin:0,
            loop:true,
            autoWidth:true,
            items:6,
          });
        });
      });
      $("#flickr_widget").append('<div class="col-lg-12"><div class="row"><div id="flickrfeed" class="owl-carousel"></div></div></div>');
    });
  }
});
</script>
